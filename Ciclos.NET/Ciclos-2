
using System;

class Program
{
    static void Main()
    {
        int suma = 0;

    
        for (int i = 1; i <= 20; i++)
        {
            suma += i;
        }


        Console.WriteLine("La suma de los primeros 20 nÃºmeros naturales ingresados  es de: " + suma);

     
        Console.ReadKey();
    }
}
